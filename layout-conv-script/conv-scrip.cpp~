#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <iostream>
using namespace std;

void parse_line(char *);
int read_line(char *line);

FILE *in, *out;

int main()
{
    in = fopen("in", "r");
	out = fopen("keys-out", "w");

	char line[20];
	char *former, *latter;
	int ret=1;

	while(ret)
    {
		ret=read_line(line);
		if(ret!=0)
		{
			parse_line(line);	
		}
    }

	return 0;
}

void parse_line(char *line, char*former, char *latter)
{
	int len=strlen(line);
	for(int i=len; i>=0; i++)
	{
		if(line[i]==' ')
		{
			line[i]='\0';
			latter=&line[i]+1;
			former=line;
		}
	}
	
	return; 
}

int read_line(char *line)
{

	int stop_sign=1, ret=1, i=0;
	char scan_var=0;
    while(scan_var!='\n')
	{
		ret=fscanf(in, "%c", &scan_var);
		if(ret==EOF)
		{
			stop_sign=0;
			return stop_sign;
		}
		if(scan_var=='\n') line[i]='\0';
		else line[i]=scan_var;
		i++;
	}
	return stop_sign;
}
